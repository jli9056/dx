package com.dixin.hibernate;

import java.util.Set;

/**
 * Customer entity.
 * 
 *
 */
public class Customer extends AbstractCustomer implements java.io.Serializable {

	// Constructors

	/**
	 * 
	 */
	private static final long serialVersionUID = -1566543787306641248L;

	/** default constructor */
	public Customer() {
	}

	/** minimal constructor */
	public Customer(String name, Integer sex, String phone) {
		super(name, sex, phone);
	}

	/** full constructor */
	public Customer(String name, Integer sex, String phone, String address,
			String postcode, String comment, Set orders) {
		super(name, sex, phone, address, postcode, comment, orders);
	}

}
